{
  "name": "design-pattern",
  "version": "1.0.0",
  "description": "A repository for Design Patterns",
  "author": "Bhargav Mantha",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=10.2.3"
  },
  "repository": {
    "type": "git",
    "url": " https://github.com/BhargavMantha/design-pattern"
  },
  "scripts": {
    "v": "node -v && npm -v",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:clear": "jest --clearCache",
    "test": "tools/run-jest.sh",
    "test:watch": "tools/run-jest.sh --watch",
    "test:cov": "tools/run-jest.sh --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/cache-manager": "2.1.1",
    "@nestjs/common": "10.3.0",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.3.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.3.8",
    "@types/lodash": "^4.14.202",
    "cache-manager": "^5.4.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "lodash": "^4.17.21",
    "merge-deep": "3.0.3",
    "module-alias": "2.2.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.2.1",
    "@nestjs/devtools-integration": "^0.1.5",
    "@nestjs/schematics": "10.0.3",
    "@nestjs/testing": "10.3.0",
    "@types/express": "4.17.21",
    "@types/jest": "^29.5.11",
    "@types/merge-deep": "3.0.3",
    "@types/node": "20.10.0",
    "@types/supertest": "2.0.16",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@typescript-eslint/parser": "6.15.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.0",
    "husky": "8.0.3",
    "jest": "^29.7.0",
    "node": "20.10.0",
    "prettier": "3.1.1",
    "source-map-support": "0.5.21",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tsconfig-paths-jest": "0.0.1",
    "typescript": "5.3.3"
  },
  "_moduleAliases": {
    "@root": "dist",
    "@utils": "dist/utils"
  }
}
